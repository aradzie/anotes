{
  "name": "@notatki/parser",
  "version": "0.0.3",
  "description": "A collection of tools for creating and syncing Anki notes from simple text files.",
  "keywords": [
    "anki",
    "flashcards",
    "spaced-repetition"
  ],
  "homepage": "https://github.com/aradzie/notatki#readme",
  "bugs": {
    "url": "https://github.com/aradzie/notatki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aradzie/notatki.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "devDependencies": {
    "peggy": "^5.0.6"
  },
  "scripts": {
    "compile": "peggy --format=es --dts --output=parser$.js --allowed-start-rules=Start,NoteList,ModelList,Template ./parser.peggy",
    "pretest": "npm run compile",
    "test": "node --test '**/*.test.js'"
  },
  "publishConfig": {
    "access": "public"
  }
}
